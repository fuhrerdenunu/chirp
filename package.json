{
  "name": "chirp-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "concurrently -n server,client \"npm --prefix server run dev\" \"npm --prefix client run dev\"",
    "server": "npm --prefix server start",
    "client": "npm --prefix client start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
